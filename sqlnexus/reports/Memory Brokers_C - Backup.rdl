      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>	
	if OBJECT_ID ('tbl_dm_os_memory_brokers') is not null 
begin

with MyRuntime
as (
SELECT     ROW_NUMBER() OVER (ORDER BY runtime) AS 'RowNumber', runtime  from (select distinct runtime from tbl_dm_os_memory_brokers) t)
select RowNumber, RunTime from MyRuntime 
where RowNumber = (select MAX(rowNumber) from MyRuntime)
end
else
begin
		select cast (1 as int)  'RowNumber', CAST ('1990-01-01' as datetime)  'runtime'
end</CommandText>
