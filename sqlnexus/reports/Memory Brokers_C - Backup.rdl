        <Field Name="pool_id">
          <DataField>pool_id</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>if OBJECT_ID ('tbl_dm_os_memory_brokers') is not null
begin
--declare @rownumber int=7
declare @runtime datetime
if (@RowNumber is null)
begin
	select @runtime = MAX (runtime) from tbl_dm_os_memory_brokers
end
else
begin
	  select @runtime = runtime from (SELECT     ROW_NUMBER() OVER (ORDER BY runtime) AS 'RowNumber', runtime  from (select distinct runtime from tbl_dm_os_memory_brokers) t   ) MyRunTime  where RowNumber = @RowNumber
end

select  
runtime,
pool_id,
memory_broker_type,
allocations_kb,
allocations_kb_per_sec,
predicted_allocations_kb,
target_allocations_kb,
future_allocations_kb,
overall_limit_kb,
last_notification
from tbl_dm_os_memory_brokers where runtime = @runtime
end</CommandText>
        <QueryParameters>
          <QueryParameter Name="@RowNumber">
            <Value>=Parameters!RowNumber.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
