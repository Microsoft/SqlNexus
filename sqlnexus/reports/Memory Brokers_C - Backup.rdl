    <DataSet Name="DataSet_SQL_CPU">
      <Fields>
        <Field Name="EventTime">
          <DataField>EventTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="record_id">
          <DataField>record_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="system_idle_cpu">
          <DataField>system_idle_cpu</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="sql_cpu_utilization">
          <DataField>sql_cpu_utilization</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="nonsql_cpu_utilization">
          <DataField>nonsql_cpu_utilization</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>IF OBJECT_ID ('tbl_SQL_CPU_HEALTH') IS NOT NULL 
  SELECT DISTINCT (SELECT TOP 1 EventTime FROM  tbl_SQL_CPU_HEALTH cpu2 WHERE cpu1.record_id = cpu2.record_id) AS EventTime, 
    record_id, system_idle_cpu, sql_cpu_utilization, 100 - sql_cpu_utilization - system_idle_cpu AS nonsql_cpu_utilization 
  FROM tbl_SQL_CPU_HEALTH cpu1
  WHERE 
    (EventTime &gt;= @StartTime OR @StartTime IS NULL)
    AND (EventTime &lt;= @EndTime OR @EndTime IS NULL)
  ORDER BY EventTime
ELSE
  SELECT GETDATE() AS EventTime, 0 AS record_id, 0 AS system_idle_cpu, 0 AS sql_cpu_utilization, 0 AS nonsql_cpu_utilization</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartTime">
            <Value>=Parameters!StartTime.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndTime">
            <Value>=Parameters!EndTime.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
